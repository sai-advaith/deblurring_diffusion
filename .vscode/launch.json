            "args": ["$MODEL_FLAGS", "--classifier_scale", "1.0", "--classifier_path", "models/64x64_classifier.pt", "--classifier_depth", "4", "--model_path", "models/64x64_diffusion.pt", "$SAMPLE_FLAGS"]
    
